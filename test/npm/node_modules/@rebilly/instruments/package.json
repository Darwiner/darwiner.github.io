{
    "name": "@rebilly/instruments",
    "version": "9.75.3",
    "author": "Rebilly",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "unpkg": "dist/index.min.js",
    "files": [
        "dist",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "concurrently yarn:type:check \"vite build\" yarn:generate:type-declaration",
        "dev": "vite build --watch",
        "test:update-snapshot": "vitest -u",
        "test:unit": "vitest run",
        "test:unit:watch": "vitest",
        "type:check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "generate:type-declaration": "dts-bundle-generator --project tsconfig.declaration.json --no-check --no-banner --out-file dist/index.d.ts src/index.d.ts"
    },
    "devDependencies": {
        "@rebilly/api-metadata": "*",
        "@rebilly/eslint-config": "*",
        "@rebilly/risk-data-collector": "*",
        "@types/lodash.camelcase": "^4.3.7",
        "@types/lodash.clonedeep": "^4.5.7",
        "@types/lodash.isequal": "^4.5.6",
        "@types/lodash.kebabcase": "^4.1.7",
        "@types/lodash.merge": "^4.6.7",
        "@vue/reactivity": "^3.2.39",
        "ajv": "^8.17.1",
        "axios": "^1.7.5",
        "component-emitter": "^2.0.0",
        "concurrently": "^8.2.2",
        "core-js": "^3.38.0",
        "dts-bundle-generator": "^9.5.1",
        "jsdom": "^21.1.1",
        "jwt-decode": "^3.1.2",
        "lodash": "^4.17.21",
        "msw-when-then": "^1.5.1",
        "msw": "^1.2.1",
        "popostmate": "2.0.0",
        "rebilly-js-sdk": "*",
        "typescript": "^5.4.5",
        "values.js": "^2.0.0",
        "vite-plugin-package-version": "^1.1.0",
        "vite": "^5.4.1",
        "vitest-canvas-mock": "^0.3.3",
        "vitest-fail-on-console": "^0.7.0",
        "vitest": "^1.6.0"
    }
}
